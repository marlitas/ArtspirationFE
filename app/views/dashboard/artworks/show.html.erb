<h1> <%= @artwork.title %> </h1>
<div class="large-frame">
  <img src= <%= "#{@artwork.url}" %> alt= <%= "#{@artwork.title}" %> >
</div>
<div class="row">
  <div class="col-6">
    <h3><%= link_to @artwork.artist, @artwork.artist_url %></h3>
  </div>
  <div class="col-6">
    <h3>Added to Liked | <%= link_to 'Remove', "/dashboard/artworks/#{@artwork.id}", method: :delete %></h3>
  </div>
</div>
